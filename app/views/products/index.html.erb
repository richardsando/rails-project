<p id="notice"><%= notice %></p>

<h1>Products</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Price</th>
      <th>Artist</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= link_to product.name, product %>  </td>
        <td><%= product.description %></td>
        <td><%= product.price %></td>
        <td><%= link_to product.profile.username, product.profile %></td>
      
      </tr>
    <% end %>
  </tbody>
</table>

<br>

